{
  "name": "gigacounts",
  "author": "GIGA",
  "version": "0.1.0",
  "description": "Gigacounts Application",
  "private": true,
  "scripts": {
    "dev": "react-scripts start",
    "dev-https": "yarn start-https",
    "start": "react-scripts start",
    "start-https": "cross-env HTTPS=true SSL_CRT_FILE=./.cert/cert.pem SSL_KEY_FILE=./.cert/key.pem react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "prettier": "prettier --write ./src/**/*.{ts,tsx,scss}",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "precommit": "yarn prettier && yarn lint:fix",
    "serve": "serve -s build",
    "serve-https": "serve build --ssl-cert ./.cert/cert.pem --ssl-key ./.cert/key.pem",
    "clean-branches-windows": "git branch | %{ $_.Trim() } | ?{ $_ -ne 'master' } | ?{ $_ -ne 'release'} | %{ git branch -D $_ }",
    "clean-branches-linux": "git for-each-ref --format '%(refname:short)' refs/heads | grep -v 'master\\|main' | xargs git branch -D"
  },
  "engines": {
    "node": ">=16.14.0 <17.0.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "none",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100,
    "tabWidth": 2
  },
  "pre-commit": [
    "prettier",
    "lint:fix"
  ],
  "dependencies": {
    "@azure/msal-browser": "^2.37.1",
    "@azure/msal-react": "^1.5.8",
    "@carbon/charts-react": "^1.11.13",
    "@carbon/colors": "^11.17.1",
    "@carbon/grid": "11.17.1",
    "@carbon/icons-react": "^11.22.1",
    "@carbon/pictograms-react": "^11.46.0",
    "@carbon/react": "1.34.0",
    "@carbon/styles": "1.34.1",
    "@fullcalendar/core": "^6.1.4",
    "@hookform/resolvers": "^2.9.11",
    "@reduxjs/toolkit": "^1.9.2",
    "@web3-onboard/coinbase": "2.2.5",
    "@web3-onboard/gas": "2.1.8",
    "@web3-onboard/gnosis": "2.1.10",
    "@web3-onboard/injected-wallets": "2.9.0",
    "@web3-onboard/react": "2.8.4",
    "@web3-onboard/trust": "2.0.4",
    "@web3-onboard/walletconnect": "2.4.1",
    "axios": "^1.3.3",
    "change-case": "^4.1.2",
    "d3": "^7.8.5",
    "d3-cloud": "^1.2.5",
    "d3-sankey": "^0.12.3",
    "date-fns": "^2.29.3",
    "ethers": "^5.7.0",
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "leaflet": "1.9.4",
    "leaflet-draw": "1.0.4",
    "leaflet-fullscreen": "1.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "notistack": "3.0.1",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.43.1",
    "react-i18next": "^12.1.5",
    "react-joyride": "^2.5.4",
    "react-lazy-load-image-component": "^1.5.6",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^2.1.0",
    "react-leaflet-draw": "^0.20.4",
    "react-modern-drawer": "^1.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.8.1",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "web-vitals": "^3.1.1",
    "worker-loader": "^3.0.8",
    "xlsx": "^0.18.5",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/autosuggest-highlight": "^3.2.0",
    "@types/carbon__colors": "^10.31.0",
    "@types/carbon__icons-react": "^11.20.0",
    "@types/carbon__pictograms-react": "^11.45.0",
    "@types/leaflet": "1.9.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.0",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.4",
    "sass": "1.63.6",
    "sass-loader": "13.3.2",
    "typescript": "4.9.5"
  },
  "overrides": {
    "nth-check": "2.1.1"
  }
}
